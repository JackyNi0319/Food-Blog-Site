---
let {titles} = Astro.props;
let {caseAbout} = Astro.props;
let {caseFood} = Astro.props;
let {caseContact} = Astro.props;

import '../styles/style.css';
import '@fontsource/roboto-slab'
---

<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <title>{titles}</title>

    <link href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@400;500&display=swap" rel="stylesheet">
</head>

<body>
    <div class="spacer"> </div>
    <div class="header">
        <div class="logo"><a href="index.html">⛩️ JP Cuisine</a></div>
        <ul id="nav-bar__slider" class="nav-bar__animation selections nav-bar__list">
            <li class="nav-bar__item"><a href={caseAbout}>About</a></li>
            <li class="nav-bar__item"><a href={caseFood}>Food</a></li>
            <li class="nav-bar__item"><a href={caseContact}>Contact</a></li>
        </ul>

        <button hamburger-button class="menu-bar button-style-off">
            <div class="menu-bar__line1"></div>
            <div class="menu-bar__line2"></div>
            <div class="menu-bar__line3"></div>
        </button>
    </div>

    <slot></slot>

    <!--Adds animation for menu bar-->
    <script>
        import '../scripts/app.js';
    </script>
</body>
</html>